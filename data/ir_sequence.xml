<odoo>
    <!-- Sequence for Sales Team Target -->
    <record id="seq_sales_team_target" model="ir.sequence">
        <field name="name">Sales Team Target</field>
        <field name="code">sales.team.target</field>
        <field name="prefix">STT</field>
        <field name="padding">4</field>
        <field name="number_next">1</field>
        <field name="company_id" eval="False"/>
    </record>
        <record id="seq_sales_target" model="ir.sequence">
        <field name="name">Sales Target</field>
        <field name="code">sales.target</field>
        <field name="prefix">SPT</field>
        <field name="padding">4</field>
        <field name="company_id" eval="False"/>
    </record>
</odoo>
